{
  "name": "@corunet/openapi-nodejs-cli",
  "version": "0.1.0",
  "description": "Generates nodejs, typescript client and Angular client methods from an OpenApi document",
  "keywords": [
    "nodejs",
    "typescript",
    "openapi",
    "angular",
    "api",
    "generator"
  ],
  "license": "Mozilla Public License 2.0",
  "main": "index.ts",
  "bin": "index.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "clean": "rm -rf build output",
    "build": "tsc",
    "postbuild": "cp src/package.production.json build/openapi-nodejs-cli/package.json && cp src/README.md build/openapi-nodejs-cli/README.md && cp -R src/templates build/openapi-nodejs-cli/templates && cp LICENSE build/openapi-nodejs-cli/LICENSE",
    "start": "node build/openapi-nodejs-cli/index.js",
    "format": "prettier --write .",
    "lint": "eslint src/**/*.ts",
    "fix": "eslint src/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/corunet/openapi-nodejs-cli.git"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.21",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "prettier": "^2.6.0",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "commander": "^9.0.0",
    "handlebars": "^4.7.7",
    "yaml": "^1.10.2"
  }
}
